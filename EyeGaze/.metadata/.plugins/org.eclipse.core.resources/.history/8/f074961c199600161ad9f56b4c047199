import java.awt.FlowLayout;
//Has GUI properties (minimize, maximize, buttons).
import javax.swing.JFrame;
//Allows you to output text on the screen
import javax.swing.JLabel;

//Secondary inherits properties of JFrame
public class secondary extends JFrame{

		private JLabel item1;
		
		public secondary(){
			//Adds title to GUI
			super("Stimulus Application");
			//Gives us default layout
			setLayout(new FlowLayout());
			
			item1 = new JLabel("This is a sentence");
			//adds text that shows up when you hover over label
			item1.setToolTipText("This is gonna show up on hover");
			//actually adds it to screen
			add(item1);
			
		}
}
